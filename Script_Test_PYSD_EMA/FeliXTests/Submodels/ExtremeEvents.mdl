********************************************************
	.extremeevents
********************************************************~
		<?xml version="1.0" encoding="UTF-8"?>
		<richtext version="1.0.0.0" xmlns="http://www.wxwidgets.org">
		  <paragraphlayout textcolor="#000000" fontpointsize="9" fontfamily="70" \
		fontstyle="90" fontweight="400" fontunderlined="0" fontface="Segoe UI" \
		alignment="1" parspacingafter="10" parspacingbefore="0" linespacing="10" \
		margin-left="5,4098" margin-right="5,4098" margin-top="5,4098" \
		margin-bottom="5,4098">
		    <paragraph>
		      <text></text>
		    </paragraph>
		  </paragraphlayout>
		</richtext>
		
	|

New Z=
	(Threshold Z for Extreme Temperatures-Temperature Change from Preindustrial) /StDev Temperature
	~	DegreesC
	~		|

Average Poisson Event Generation=
	past quantity held/Window Avg Time
	~	Events/Year
	~		|

Climate Events in Memory= INTEG (
	Perception of Events-Forgetting of Events,
		INIT Climate Events in Memory)
	~	Event
	~		|

Climate Risk Perception Delay[noEd]=
	10 + STEP(Climate Risk Perception Delay NoEd Variation-10, 2020) ~~|
Climate Risk Perception Delay[primary]=
	8 + STEP(Climate Risk Perception Delay Primary Variation-8, 2020) ~~|
Climate Risk Perception Delay[secondary]=
	5 + STEP(Climate Risk Perception Delay Secondary Variation-5, 2020) ~~|
Climate Risk Perception Delay[tertiary]=
	3 + STEP(Climate Risk Perception Delay Tertiary Variation-3, 2020)
	~	Year
	~	10, 8, 5, 3
	|

Climate Risk Perception Delay NoEd Variation=
	10
	~	Year
	~		|

Climate Risk Perception Delay Primary Variation=
	8
	~	Year
	~		|

Climate Risk Perception Delay Secondary Variation=
	5
	~	Year
	~		|

Climate Risk Perception Delay Tertiary Variation=
	3
	~	Year
	~		|

Delayed Poisson Event Generation=
	DELAY FIXED(Number of Future Events, Window Avg Time , Number of Future Events )
	~	Events/Year
	~		|

Education:
	noEd, primary, secondary, tertiary
	~	
	~		|

Event Choice Switch=
	1
	~	Dmnl
	~		|

Event Perception Threshold=
	0.5+STEP(Event Perception Threshold Variation-0.5, 2020)
	~	Events/Year
	~	A threshold (Beta) that an events value  must be higher than to register \
		in the public psyche and eneter the pool.
	|

Event Perception Threshold Variation=
	0.5
	~	Events/Year
	~	A threshold (Beta) that an events value  must be higher than to register \
		in the public psyche and eneter the pool.
	|

Event Series=
	IF THEN ELSE(
	   Event Choice Switch=0,
	   Number of Future Events,
	   Window Event Avg Difference
	)
	~	Events/Year
	~		|

Extreme Event Mean=
	MAX(
	Extreme Event Ratio*Reference Extreme Event Mean*Temperature Scale Factor,
	Minimum Extreme Event Mean)
	~	Events/Year
	~		|

Extreme Event Ratio=
	(1-StdNormal Approximation)/(1-Upper Tail Threshold)
	~	Dmnl
	~	 Relative number of extreme events to preindustrial extreme events
	|

Filter Date=
	2017
	~	Year
	~		|

Forgetting of Events=
	Climate Events in Memory/Time to forget climate events
	~	Events/Year
	~		|

INIT Climate Events in Memory=
	0
	~	
	~		|

inverse CDF=
	T-(2.51552+ 0.802853*T + 0.010328*T*T)/(1 + 1.43279*T + 0.189269*T*T + 0.001308*T*T*\
		T)
	~	Dmnl
	~	Formula 26.2.23 from Abramowitz and Stegun
		comparison from matlab:
		>> x=norminv([0,0.9],0,1)
		
		x =
		
		      -Inf    1.2816
		
		>> x=norminv([0,0.95],0,1)
		
		x =
		
		      -Inf    1.6449
	|

Maximum Number of Extreme Events=
	1000
	~	Events/Year
	~		|

Mean Temperature for Events=
	3
	~	DegreesC
	~	0.964
	|

Minimum Extreme Event Mean=
	0.1
	~	Events/Year
	~	To prevent negative mean value for mean number of extreme events per year
	|

Minimum Number of Extreme Events=
	0
	~	Events/Year
	~		|

Number of Future Events=
	IF THEN ELSE( 
	   Time<=Filter Date ,
	   Reference Extreme Event Mean ,
	   Poisson Arrivals
	)
	~	Events/Year
	~		|

past quantity held= INTEG (
	Number of Future Events-Delayed Poisson Event Generation,
		Number of Future Events*Window Avg Time)
	~	Events/Year
	~		|

Perceived Risk[Education]=
	SMOOTH(Climate Events in Memory,Climate Risk Perception Delay[Education])
	~	Event
	~	Beckage et al. uses linear, cubic and logistic function to relate events \
		to perceived risk. They show that the greates variety is obtained with the \
		logistic function form, therefore I choose to use only the logistic \
		function. They have a final term L/2+0.5 in the equation. I keep using it \
		as they did.
	|

Perception of Events=
	IF THEN ELSE(
	   Event Series > Event Perception Threshold,
	    Event Series ,
	    0
	)
	~	Events/Year
	~		|

PI=
	3.14159
	~	Dmnl
	~		|

Poisson Arrivals=
	RANDOM POISSON(Minimum Number of Extreme Events, Maximum Number of Extreme Events, Extreme Event Mean\
		, 
	Poisson Distribution Shift, Poisson Distribution Stretch, S Stream ID)
	~	Events/Year
	~		|

Poisson Distribution Shift=
	0
	~	Events/Year
	~		|

Poisson Distribution Stretch=
	1
	~	Dmnl
	~		|

Reference Extreme Event Mean=
	20
	~	Events/Year
	~		|

S Stream ID=
	5
	~	Dmnl
	~		|

SA Climate Risk Perception Delay[Education]=
	10, 8, 5, 3
	~	Year
	~		|

SA Time to forget climate events=
	2
	~	Year
	~	The time it takes for a memory to fade from public memory.
	|

StDev Temperature=
	SQRT(Variance Temperature for Events)
	~	Days/Year
	~		|

StdNormal Approximation=
	1-(1/(2*PI)^0.5)*( (EXP((-1)*(New Z*New Z)/(2))) / (0.226+0.64*New Z+0.33*((New Z*New Z\
		+3)^0.5) ))
	~	Dmnl
	~		|

T=
	(-2* LN( 1-Upper Tail Threshold ) )^0.5
	~	Dmnl
	~		|

Temperature Scale Factor=
	1
	~	Dmnl
	~		|

Threshold Z for Extreme Temperatures=
	inverse CDF*StDev Temperature+Mean Temperature for Events
	~	DegreesC
	~	90% right position @ mean 0.964 var 16.76
	|

Time to forget climate events=
	2 + STEP(SA Time to forget climate events-2, 2020)
	~	Year
	~	The time it takes for a memory to fade from public memory.
	|

Upper Tail Threshold=
	0.99
	~	Dmnl
	~		|

Variance Temperature for Events=
	48
	~	DegreesC*DegreesC
	~	16.763
		
		GEOPHYSICAL RESEARCH LETTERS, VOL. 39, L14707, doi:10.1029/2012GL052459, 2012
		
		The shifting probability distribution of global daytime
		and night-time temperatures
		Markus G. Donat1 and Lisa V. Alexander1,2
		
		Figure 2 A
	|

Window Avg Time=
	1
	~	Year
	~		|

Window Event Avg Difference=
	Number of Future Events-Average Poisson Event Generation
	~	Events/Year
	~		|

